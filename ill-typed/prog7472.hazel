type expr = 
  + VarX
  + VarY
  + Sine(expr)
  + Cosine(expr)
  + Average(expr, expr)
  + Times(expr, expr)
  + Thresh(expr, expr, expr, expr)
 in let pi = 4. *. atan(1.) in let exprToString = fun e -> case e 
  | VarX => e
  | Sine(e') => sin(pi * e')
  | Cosine(e') => cos(pi * e')
  | Average => e' + e'
  | Times => e' * e'
  | Thresh => ?
end in ?
