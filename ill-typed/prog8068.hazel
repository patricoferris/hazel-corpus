type expr = 
  + VarX
  + VarY
  + Sine(expr)
  + Cosine(expr)
  + Average(expr, expr)
  + Times(expr, expr)
  + Thresh(expr, expr, expr, expr)
 in let exprToString = fun e -> case e 
  | VarX => "VarX"
  | VarY => "VarY"
  | Sine => "Sine"
  | Cosine => "Cosine"
  | Average => "Average"
  | Times => "Times"
  | Thresh => "Thresh"
end in ?
