type expr = 
  + VarX
  + VarY
  + Sine(expr)
  + Cosine(expr)
  + Average((expr, expr))
  + Times((expr, expr))
  + Thresh((expr, expr, expr, expr))
 in
let exprToString = fun e -> case e 
  | VarX => "VarX"
  | VarY => "VarY"
  | Sine(e) => "Sine"
  | Cosine(e) => "Cosine"
  | Average(e) => "Average"
  | Times(e) => "Times"
  | Thresh(e) => "Thresh"
end in
?
