type expr = 
  + VarX
  + VarY
  + Sine(expr)
  + Cosine(expr)
  + Average(expr, expr)
  + Times(expr, expr)
  + Thresh(expr, expr, expr, expr)
 in let exprToString : expr -> String = fun e -> case e 
  | VarX(x) => "x"
  | VarY(y) => "y"
  | Sine(s) => ^("sin (pi*")(^(exprString(e))(")"))
end in ?
